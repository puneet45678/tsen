<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="1" author="Himanshu">
        <createTable tableName="ikarus_nest_additional_session_info">
            <column name="external_user_id" type="varchar(45)"/>
            <column name="session_handle" type="varchar(36)">
                <constraints primaryKey="true"/>
            </column>
            <column name="sec_ch_ua" type="varchar(128)"/>
            <column name="sec_ch_ua_mobile" type="varchar(255)"/>
            <column name="sec_ch_ua_platform" type="varchar(255)"/>
            <column name="Sec_Fetch_Dest" type="varchar(255)"/>
            <column name="Sec_Fetch_Mode" type="varchar(255)"/>
            <column name="Sec_Fetch_Site" type="varchar(255)"/>
            <column name="User_Agent" type="varchar(255)"/>
            <column name="client_ip" type="varchar(45)"/>
        </createTable>
    </changeSet>

    <changeSet id="2" author="Himanshu">
        <createTable tableName="User_device_mapping">
            <column name="user_id" type="varchar(255)">
                <constraints primaryKey="true"/>
            </column>
            <column name="fingerprint_id" type="varchar(255)">
                <constraints primaryKey="true"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="3" author="Himanshu">
        <createTable tableName="permissions">
            <column name="permission" type="varchar(255)">
                <constraints primaryKey="true"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>

